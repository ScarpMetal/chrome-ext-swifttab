{
  "manifest_version": 2,
  "name": "SwiftTab",
  "description":
    "Personal new tab page featuring links from your Bookmarks Bar",
  "version": "0.2.0",
  "homepage_url": "https://github.com/joshbuchea/chrome-ext-swifttab",
  "author": "Josh Buchea",
  "permissions": ["bookmarks", "chrome://favicon/"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": [
    {
      "src": "assets/icon-16.png",
      "sizes": "16x16",
      "type": "image/png"
    },
    {
      "src": "assets/icon-32.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "assets/icon-128.png",
      "sizes": "128x128",
      "type": "image/png"
    }
  ]
}
